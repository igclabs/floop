{
    "name": "igclabs/floop",
    "description": "An autonomous feedback loop for Laravel. Testers submit feedback, AI agents fix the code. No database, no tickets, no waiting.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.0",
        "laravel/pint": "^1.27",
        "larastan/larastan": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "IgcLabs\\Floop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IgcLabs\\Floop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IgcLabs\\Floop\\FloopServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
